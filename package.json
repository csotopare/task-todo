{
	"name": "webpack-starter",
	"version": "1.0.0",
	"description": "A light foundation for your next frontend project based on webpack.",
	"scripts": {
		"lint": "npm run lint:scripts",
		"lint:scripts": "eslint src",
		"build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --colors",
		"start": "webpack-dev-server --open --config webpack/webpack.config.dev.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wbkd/webpack-starter.git"
	},
	"keywords": [
		"webpack",
		"startkit",
		"frontend",
		"es6",
		"javascript",
		"webdev"
	],
	"author": "Carlos.Soto",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.12.8",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.12.7",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.1",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.3.2",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.1",
		"eslint": "^7.14.0",
		"eslint-loader": "^4.0.2",
		"file-loader": "^6.1.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.0",
		"jsdom": "^16.4.0",
		"mini-css-extract-plugin": "^1.3.1",
		"style-loader": "^2.0.0",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.1.4"
	},
	"dependencies": {
		"@babel/polyfill": "^7.11.5",
		"core-js": "^3.6.5"
	}
}
